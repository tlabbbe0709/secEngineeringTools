| metadata type=sourcetypes index=*
| eval timeSince=now()-recentTime,
eval status=case(timeSince>86400 AND timeSince<=604800,"Greater than 24h Delay", timeSince>604800 AND timeSince<=2592000, "Greater than 1 Week Delay",timeSince>2592000, "Greater than 1 Month Delay",1==1,"Okay"),
eval firstTime=strftime(firstTime,"%c"),
eval recentTime=strftime(recentTime,"%c"),
eval timeSince=tostring(timeSince,"duration")
| search status="Okay"
| table firstTime recentTime timeSince sourcetype totalCount status